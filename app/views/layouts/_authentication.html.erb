<% if current_user %>
  <li>Signed in as <%= current_user.email %></li>
  <li><%= link_to 'Logout', logout_path, method: :delete %></li>
  <li><%= link_to 'My Cars', my_cars_path %></li>
<% else %>
  <li><%= link_to 'Signup', new_user_path %></li>
  <li><%= link_to 'Login', login_path %></li>
<% end %>
